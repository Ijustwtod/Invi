<Window x:Class="Invi.Views.Authorization.WebBrowserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Invi.Views.Authorization"
        mc:Ignorable="d"
        Title="WebBrowserWindow" Height="900" Width="800">
    <Grid Margin="0,0,0,0">
        <WebBrowser x:Name="WebBrowser" LoadCompleted="WebBrowser_LoadCompleted"/>
    </Grid>
</Window>
